[user]
	name = masquee
	email = okayanz@outlook.com
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
[core]
	quotepath = false
	mergeoptions = --no-edit
	editor = /usr/bin/vim
	trustctime = false
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[pull]
	rebase = true
[mergetool]
	keepBackup = false
[includeIf "gitdir:~/Projects/trip/"]
	path = ~/.gitconfig_trip
[url "git@git.dev.sh.ctripcorp.com:ctripgo"]
	insteadOf = https://github.com/ctripcorp
[url "git@git.dev.sh.ctripcorp.com:servicemesh"]
	insteadOf = http://git.dev.sh.ctripcorp.com/servicemesh

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
