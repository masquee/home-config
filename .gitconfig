[user]
	name = masquee
	email = okayanz@outlook.com
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
[core]
	quotepath = false
	mergeoptions = --no-edit
	editor = /usr/bin/vim
	trustctime = false
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[pull]
	rebase = true
[mergetool]
	keepBackup = false
[includeIf "gitdir:~/Projects/trip/"]
	path = ~/.gitconfig_trip
[url "git@git.dev.sh.ctripcorp.com:ctripgo"]
	insteadOf = https://github.com/ctripcorp
[url "git@git.dev.sh.ctripcorp.com:servicemesh"]
	insteadOf = http://git.dev.sh.ctripcorp.com/servicemesh
[url "git@git.dev.sh.ctripcorp.com:framework"]
	insteadOf = http://git.dev.sh.ctripcorp.com/framework
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
